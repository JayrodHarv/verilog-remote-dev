#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5e13862c83a0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5e13862c8530 .scope module, "counter_tb" "counter_tb" 3 3;
 .timescale -9 -12;
P_0x5e13862b37a0 .param/l "N" 1 3 5, +C4<00000000000000000000000000001000>;
P_0x5e13862b37e0 .param/l "T" 1 3 6, +C4<00000000000000000000000000001010>;
v0x5e13862de9c0_0 .var "clk", 0 0;
v0x5e13862dea80_0 .net "count", 7 0, v0x5e13862de6a0_0;  1 drivers
v0x5e13862deb50_0 .var "en", 0 0;
v0x5e13862dec50_0 .var "rst", 0 0;
E_0x5e13862c75c0 .event negedge, v0x5e13862b7b10_0;
S_0x5e13862b77d0 .scope module, "uut" "counter" 3 15, 4 3 0, S_0x5e13862c8530;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /OUTPUT 8 "count";
P_0x5e13862b79b0 .param/l "N" 0 4 4, +C4<00000000000000000000000000001000>;
v0x5e13862b7b10_0 .net "clk", 0 0, v0x5e13862de9c0_0;  1 drivers
v0x5e13862de6a0_0 .var "count", 7 0;
v0x5e13862de780_0 .net "en", 0 0, v0x5e13862deb50_0;  1 drivers
v0x5e13862de850_0 .net "rst", 0 0, v0x5e13862dec50_0;  1 drivers
E_0x5e13862c7060 .event posedge, v0x5e13862de850_0, v0x5e13862b7b10_0;
    .scope S_0x5e13862b77d0;
T_0 ;
    %wait E_0x5e13862c7060;
    %load/vec4 v0x5e13862de850_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5e13862de6a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5e13862de780_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5e13862de6a0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5e13862de6a0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5e13862c8530;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e13862de9c0_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e13862de9c0_0, 0, 1;
    %delay 5000, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5e13862c8530;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e13862dec50_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e13862dec50_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5e13862c8530;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e13862deb50_0, 0, 1;
    %pushi/vec4 3, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5e13862c75c0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e13862deb50_0, 0, 1;
    %delay 100000, 0;
    %vpi_call/w 3 42 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5e13862c8530;
T_4 ;
    %vpi_call/w 3 46 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000001, " ns", 32'sb00000000000000000000000000001000 {0 0 0};
    %vpi_call/w 3 47 "$monitor", "time=%t clk=%b rst=%b en=%b count=%2d", $time, v0x5e13862de9c0_0, v0x5e13862dec50_0, v0x5e13862deb50_0, v0x5e13862dea80_0 {0 0 0};
    %vpi_call/w 3 48 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call/w 3 49 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5e13862c8530 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
